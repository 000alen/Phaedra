{
  "name": "phaedra-monorepo",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "app/",
    "gh-pages/",
    "packages/**"
  ],
  "scripts": {
    "start": "npm run start --workspace=app",
    "start:gh-pages": "npm run start --workspace=gh-pages",
    "start:content": "npm run start --workspace=packages/phaedra-content",
    "start:layout": "npm run start --workspace=packages/phaedra-layout",
    "build:content": "npm run build --workspace=packages/phaedra-content",
    "build:layout": "npm run build --workspace=packages/phaedra-layout",
    "example:content": "npm run start --workspace=packages/phaedra-content/example/",
    "example:layout": "npm run start --workspace=packages/phaedra-layout/example/"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-standard": "^5.0.0",
    "prettier": "^2.5.1"
  }
}